{
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "extends": [
    "skype",
    "skype/react"
  ],
  "rules": {
    "camelcase": "off",
    "@typescript-eslint/camelcase": ["error", { "properties": "never" }]
  },
  "overrides": [
    {
      "files": [
        "*.tsx"
      ],
      "rules": {
        "@typescript-eslint/explicit-function-return-type": "off"
      }
    }
  ]
}
